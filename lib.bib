  keywords = {CLINICAL,Clinical Psychology,Cognitive Psychology,DEPRESSION,distraction,DURATION,dysphoric mood,EPISODES,FOCUS,GENDER,INDUCTION,mindfulness,Psychology,PSYCHOLOGY,RESPONSES,rumination,STRATEGIES,STYLE,SYMPTOMS},
